{
  "name": "/nerv/env_standalone",
  "version": 1,
  "environment": "standalone",
  "nodes": [
    {
      "name": "agent",
      "type": "/nerv/standalone/Agent",
      "parameters": [
        {
          "name": "pkg_root",
          "value": "../../pkg"
        },
        {
          "name": "pkg_url",
          "value": "/agent.tar.gz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "server",
      "type": "/nerv/standalone/Server",
      "parameters": [
        {
          "name": "pkg_root",
          "value": "../../pkg"
        },
        {
          "name": "pkg_url",
          "value": "/server.tar.gz"
        },
        {
          "name": "root",
          "value": "../../"
        },
        {
          "name": "config_root",
          "value": "../../../../profile/ide"
        },
        {
          "name":"config_url",
          "value":"/server/config"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "file",
      "type": "/nerv/standalone/File",
      "parameters": [
        {
          "name": "pkg_root",
          "value": "../../pkg"
        },
        {
          "name": "pkg_url",
          "value": "/file.tar.gz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "webui",
      "type": "/nerv/standalone/WebUI",
      "parameters": [
        {
          "name": "pkg_root",
          "value": "../../pkg"
        },
        {
          "name": "pkg_url",
          "value": "/webui.tar.gz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "host",
      "type": "/nerv/standalone/Host",
      "parameters": [
        {
          "name": "address",
          "value": "127.0.0.1"
        }
      ]
    }
  ]
}
