{
  "name": "/nerv/env_standalone",
  "version": 1,
  "environment": "standalone",
  "inputs":[
    {
      "name":"os",
      "type":"string"
    }
  ],
  "nodes": [
    {
      "name": "agent",
      "type": "/nerv/standalone/Agent",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/agent-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "server",
      "type": "/nerv/standalone/Server",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/server-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        },
        {
          "name": "config_root",
          "value": "../../resources/config/nerv"
        },
        {
          "name": "config_url",
          "value": "/server"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "file",
      "type": "/nerv/standalone/File",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/file-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        },
        {
          "name": "config_root",
          "value": "../../resources/config/nerv"
        },
        {
          "name": "config_url",
          "value": "/file"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "webui",
      "type": "/nerv/standalone/WebUI",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/webui-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "log",
      "type": "/nerv/standalone/Log",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/log-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        },
        {
          "name": "config_root",
          "value": "../../resources/config/nerv"
        },
        {
          "name": "config_url",
          "value": "/log"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "stroe",
      "type": "/nerv/standalone/Store",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/store-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "logui",
      "type": "/nerv/standalone/LogUI",
      "parameters": [
        {
          "name": "pkg_url",
          "value": "../../pkg/logui-${os}.tgz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "host",
      "type": "/nerv/standalone/Host",
      "parameters": [
        {
          "name": "address",
          "value": "127.0.0.1"
        }
      ]
    }
  ]
}
