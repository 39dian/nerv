{
  "name": "test-service-deploy",
  "version": 1,
  "nodes": [
    {
      "name": "loadBalance",
      "type": "/middleware/OpenResty",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        },
        {
          "type": "connect",
          "target": "webServer"
        }
      ]
    },
    {
      "name": "webServer",
      "type": "/middleware/SpringBoot",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        },
        {
          "type": "connect",
          "target": "mysql"
        }
      ]
    },
    {
      "name": "mysql",
      "type": "/db/MySQL",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "host",
      "type": "/host/Host",
      "properties": {
        "ip": [
          "nginx.dev.chaosxu.com"
        ]
      }
    }
  ]
}