{
  "name": "/nerv/env_standalone",
  "version": 1,
  "environment": "standalone",
  "nodes": [
    {
      "name": "file",
      "type": "/nerv/standalone/File",
      "parameters": [
        {
          "name": "pkg_root",
          "value": "../../pkg"
        },
        {
          "name": "pkg_url",
          "value": "/file.tar.gz"
        },
        {
          "name": "root",
          "value": "../../"
        }
      ],
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "host",
      "type": "/nerv/standalone/Host",
      "parameters": [
        {
          "name": "address",
          "value": "127.0.0.1"
        }
      ]
    }
  ]
}
