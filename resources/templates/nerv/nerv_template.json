{
  "name": "nerv_template",
  "version": 1,
  "inputs": [
    {
      "name":"user",
      "type":"string"
    },
    {
      "name":"pwd",
      "type":"string"
    }
  ],
  "nodes": [
    {
      "name": "server",
      "type": "/nerv/Server",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "file",
      "type": "/nerv/File",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "webui",
      "type": "/nerv/WebUI",
      "dependencies": [
        {
          "type": "contained",
          "target": "host"
        }
      ]
    },
    {
      "name": "host",
      "type": "/nerv/Agent",
      "parameters": [
        {
          "name": "addresses",
          "value": "localhost"
        },
        {
          "name": "user",
          "value": "${user}"
        },
        {
          "name": "pwd",
          "value": "${pwd}"
        }
      ]
    }
  ]
}
